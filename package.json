  "description": "The “Get your National Insurance number by post” service allows unauthenticated users to get HMRC to send them proof of their National Insurance number by post. This is an alternative to similar functionality in the Personal Tax Account, for users who cannot pass identity verification.",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "engines": {
    "node": "^14.0.0"
  },
  "homepage": "https://github.com/hmrc/national-insurance-number-letter-spike-frontend#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm test"
    }
  },
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "name": "national-insurance-number-letter-spike-frontend",
  "nodeW3Cvalidator": {
    "suppressErrors": [],
    "suppressWarnings": []
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "stop-only"
  ],
  "quokka": {
    "babel": true,
    "env": {
      "browser": true,
      "cypress": true,
      "es6": true,
      "jest": true,
      "mocha": true,
      "node": true
    },
    "globals": {
      "afterAll": true,
      "afterEach": true,
      "beforeAll": true,
      "beforeEach": true,
      "cy": true,
      "describe": true,
      "expect": true,
      "it": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hmrc/national-insurance-number-letter-spike-frontend.git"
  },
  "scripts": {
    "browserslist": "npx update-browserslist-db@latest",
    "check": "npx npm-check -u",
    "cover": "run-s cover:report cover:merge",
    "cover:merge": "nyc merge .nyc_output coverage/coverage.json",
    "cover:report": "nyc report --reporter=lcov --reporter=text-summary",
    "coverage:e2e": "NODE_ENV=e2e npx nyc npm run cypress:run",
    "coverage:report": "NODE_ENV=test npx nyc report --compact",
    "coverage:test": "NODE_ENV=test npx nyc npm run cypress:run",
    "css": "sass app/assets/stylesheets/application.scss app/assets/stylesheets/application.min.css",
    "css:watch": "sass --watch --poll app/assets/stylesheets/application.scss app/assets/stylesheets/application.min.css",
    "cypress:clear": "cypress cache clear",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome --reporter-options reportDir='cypress/results',overwrite=false,html=true,json=true",
    "deps": "npx install-peerdeps --",
    "lint": "run-s lint:js lint:scss",
    "lint:js": "eslint --fix './app/assets/javascripts/**/*.js'",
    "lint:scss": "stylelint --fix 'app/assets/stylesheets/**/*.*ss'",
    "migrator": "sass-migrator division 'app/assets/stylesheets/**/*.scss'",
    "postcss": "postcss --replace app/assets/stylesheets/application.min.css",
    "postinstall": "run-s browserslist migrator lint css cypress:install coverage:e2e",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "precover:merge": "mv coverage/coverage-final.json .nyc_output",
    "start": "run-s cypress:open",
    "stop-only": "stop-only --folder cypress/e2e",
    "test": "run-s coverage:test"
  },
  "version": "0.106.0",
  "wallaby": {
    "autoDetect": true
  }
